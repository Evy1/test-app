vateRoute({component: Component, ...rest}) {\n    const {currentUser, loading} = useAuth()\n    return (\n        <Route {...rest} render={props=> {\n          return  currentUser ? <Component {...props} /> : <Redirect to=\"/login\"/>\n        }}></Route>\n    )\n}\n","/home/nmc/Desktop/auth-app/src/components/Login.js",[],"/home/nmc/Desktop/auth-app/src/components/ForgotPassword.js",[],"/home/nmc/Desktop/auth-app/src/context/AuthContext.js",[],"/home/nmc/Desktop/auth-app/src/components/Home/Home.js",[],"/home/nmc/Desktop/auth-app/src/components/View/View.js",[],"/home/nmc/Desktop/auth-app/src/components/Singup.js",[],"/home/nmc/Desktop/auth-app/src/components/TaskForm/TaskForm.js",["79","80"],"import React, { useState } from \"react\";\nimport { Form, Button, Container } from \"react-bootstrap\";\nimport results from \"../results\";\nimport { Link } from \"react-router-dom\";\nimport Dashboard from \"../Dashboard\";\nimport styles from \"./TaskForm.module.css\";\nimport { useAuth } from \"../../context/AuthContext\";\nimport firebase from \"../../firebase\";\nimport { useFormik } from 'formik';\n\nfunction TaskForm() {\n  const [title, setTitle] = useState();\n  const [content, setContent] = useState();\n  const [date, setDate] = useState();\n  const { currentUser } = useAuth();\n  const [message, setMessage] = useState();\n  const [error, setError] = useState();\n\n  const postHandler = (e) => {\n    e.preventDefault();\n    const data = {\n      title: title,\n      content: content,\n      date: date,\n      user: currentUser.uid,\n      status: \"pending\",\n    };\n    firebase\n      .database()\n      .ref('task')\n      .push(data)\n      .then(() => {\n        setMessage(\"Task was recorded\");\n        setTitle(\"\");\n        setContent(\"\");\n        setDate(\"\");\n      })\n      .catch((error) => {\n        setError(\"Task couldnt register \");\n      });\n  };\n  return (\n    <>\n      <Dashboard />\n      <Container>\n        <div className=\"row py-4 justify-content-center\">\n          <h2>Add a new task here</h2>\n        </div>\n        <div className=\"row\">\n          <form onSubmit={postHandler} className={styles.Form}>\n            {message && <div className={styles.Success}>{message}</div>}\n            {error && <div className={styles.Failed}>{error}</div>}\n            <Form.Group\n              id=\"title\"\n              className=\"d-flex justify-content-center flex-column text-center\"\n            >\n              <label>Task Title</label>\n              <input\n                className={styles.Input}\n                type=\"text\"\n                required\n                placeholder=\"Title\"\n                value={[title]}\n                onChange={(e) => setTitle(e.target.value)}\n              />\n            </Form.Group>\n            <Form.Group\n              id=\"content\"\n              className=\"d-flex justify-content-center flex-column text-center\"\n            >\n              <label>Task Content</label>\n              <textarea\n                className={styles.Input}\n                type=\"textarea\"\n                rows={3}\n                required\n                placeholder=\"Task\"\n                value={[content]}\n                onChange={(e) => setContent(e.target.value)}\n              />\n            </Form.Group>\n            <Form.Group\n              id=\"date\"\n              className=\"d-flex justify-content-center flex-column text-center\"\n            >\n              <label>Pick a deadline</label>\n              <input\n                className={styles.Input}\n                type=\"datetime-local\"\n                value={[date]}\n                onChange={(e) => setDate(e.target.value)}\n              ></input>\n            </Form.Group>\n            <div>\n              <Button type=\"submit\">Submit</Button>\n              <Link className=\"btn-primary p-2 ml-4\" to=\"/\">\n                Cancel\n              </Link>\n            </div>\n          </form>\n        </div>\n      </Container>\n    </>\n  );\n}\nexport default TaskForm;\n","/home/nmc/Desktop/auth-app/src/components/Navigation/LeftNavigation.js",[],"/home/nmc/Desktop/auth-app/src/firebase.js",["81"],"import * as base from 'firebase';\nimport firebase from 'firebase/app';\nimport 'firebase/auth';\n\n\nconst app = firebase.initializeApp({\n    apiKey: \"AIzaSyCHaQpuMa2fH8J98niwtPeD4s4V5K1cN8Q\",\n    authDomain: \"auth-dev-82cae.firebaseapp.com\",\n    databaseURL: \"https://auth-dev-82cae-default-rtdb.firebaseio.com\",\n    projectId: \"auth-dev-82cae\",\n    storageBucket: \"auth-dev-82cae.appspot.com\",\n    messagingSenderId: \"379584520517\",\n    appId: \"1:379584520517:web:9a98db554b28c8b626de2c\"\n})\n\nexport const auth = app.auth();\nexport default app;","/home/nmc/Desktop/auth-app/src/components/results.js",[],"/home/nmc/Desktop/auth-app/src/components/Tasks/Tasks.js",[],"/home/nmc/Desktop/auth-app/src/components/Buttons/Buttons.js",[],"/home/nmc/Desktop/auth-app/src/components/Spiner/Spiner.js",[],{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","line":9,"column":12,"nodeType":"88","messageId":"89","endLine":9,"endColumn":23},{"ruleId":"86","severity":1,"message":"90","line":6,"column":25,"nodeType":"88","messageId":"89","endLine":6,"endColumn":32},{"ruleId":"86","severity":1,"message":"91","line":3,"column":8,"nodeType":"88","messageId":"89","endLine":3,"endColumn":15},{"ruleId":"86","severity":1,"message":"92","line":9,"column":10,"nodeType":"88","messageId":"89","endLine":9,"endColumn":19},{"ruleId":"86","severity":1,"message":"93","line":1,"column":13,"nodeType":"88","messageId":"89","endLine":1,"endColumn":17},"no-native-reassign",["94"],"no-negated-in-lhs",["95"],"no-unused-vars","'currentUser' is assigned a value but never used.","Identifier","unusedVar","'loading' is assigned a value but never used.","'results' is defined but never used.","'useFormik' is de